  gradient_shades = { 1.1, 1.0, 1.0, 1.4 }
  lightborder_shade = 2.3
  lightborderstyle = 0
  contrast = 1.0
  glazestyle = 3
  glowstyle = 4
  roundness = 20
  progressbarstyle = 1
 }
}

