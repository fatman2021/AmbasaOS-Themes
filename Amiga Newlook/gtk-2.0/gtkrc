style "theme-radiobutton"	= "theme-default"
{

  engine "pixmap" 
    {
 image 
	{
            function        		= OPTION
            recolorable    		= TRUE
            state 				= NORMAL
            shadow          		= OUT
            overlay_file    		= "Check-Radio/radio-normal-off.png"
            overlay_stretch 	= FALSE
    }
 image 
	{
            function        		= OPTION
            recolorable     		= TRUE
            state 				= PRELIGHT
            shadow          		= OUT
            overlay_file    		= "Check-Radio/radio-prelight-off.png"
            overlay_stretch 	= FALSE
        }
 image 
	{
            function        		= OPTION
            recolorable     		= TRUE
            state 				= ACTIVE
            shadow          		= OUT
            overlay_file    		= "Check-Radio/radio-active-off.png"
            overlay_stretch 	= FALSE
        }
 image 
	{
            function        		= OPTION
            recolorable     		= TRUE
            state 				= INSENSITIVE
            shadow         		= OUT
            overlay_file    		= "Check-Radio/radio-insensitive-off.png"
            overlay_stretch 	= FALSE
        }
 image 
	{
            function        		= OPTION
            recolorable     		= TRUE
	    	  state 				= NORMAL
            shadow          		= IN
            overlay_file    		= "Check-Radio/radio-normal-on.png"
            overlay_stretch 	= FALSE
        }
  image 
	{
            function        		= OPTION
            recolorable     		= TRUE
	    	  state 				= PRELIGHT
            shadow          		= IN
            overlay_file    		= "Check-Radio/radio-prelight-on.png"
            overlay_stretch 	= FALSE
        }
 image 
	{
            function        		= OPTION
            recolorable     		= TRUE
	    	  state 				= ACTIVE
            shadow          		= IN
            overlay_file    		= "Check-Radio/radio-active-on.png"
            overlay_stretch 	= FALSE
        }
 image 
	{
            function        		= OPTION
            recolorable     		= TRUE
	   	  state 				= INSENSITIVE
            shadow          		= IN
            overlay_file    		= "Check-Radio/radio-insensitive-on.png"
            overlay_stretch 	= FALSE
        }
    }
}


style "theme-checkbutton"	= "theme-default"
{
    bg[INSENSITIVE]   = "#eeeeec"
    bg[NORMAL]        = "#eeeeec"
    bg[PRELIGHT]      = "#eeeeec"
    
  	engine "pixmap" 
    {
 image 
	{
        function        	= CHECK
        recolorable     	= TRUE
        state 				= NORMAL
        shadow          	= OUT
        overlay_file    	= "Check-Radio/check-normal-off.png"
        overlay_stretch 	= FALSE
    }
 image 
	{
        function        	= CHECK
        recolorable     	= TRUE
        state 				= PRELIGHT
        shadow          	= OUT
        overlay_file    	= "Check-Radio/check-prelight-off.png"
        overlay_stretch 	= FALSE
    }
